{
    "name": "typescript",
    "version": "1.0.0",
    "main": "app.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "clean": "rimraf ./build",
        "build": "npm run prisma:generate && npm run clean && tsc && tsc-alias",
        "start": "npm run build && node build/app.js",
        "dev": "nodemon",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up --build",
        "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up --build",
        "merge:schema": "node prismix.config.js && dotenv -e .env.local npx prismix && prisma format --schema=./src/Persistence/prisma/schema.prisma",
        "prisma:generate": "npm run merge:schema && dotenv -e .env.local --  npx prisma generate --schema=./src/Persistence/prisma/schema.prisma",
        "prisma:dev": "dotenv -e .env.FIXME --  npx prisma migrate dev --schema=./src/Persistence/prisma/schema.prisma",
        "prisma:deploy": "dotenv -e .env.FIXME --  npx prisma migrate deploy --schema=./src/Persistence/prisma/schema.prisma"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "",
    "devDependencies": {
        "@types/bcrypt": "^5.0.2",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/helmet": "^4.0.0",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/module-alias": "^2.0.4",
        "@types/morgan": "^1.9.9",
        "@types/node": "^20.14.9",
        "@types/nodemailer": "^6.4.15",
        "@typescript-eslint/eslint-plugin": "^7.14.1",
        "@typescript-eslint/parser": "^7.14.1",
        "eslint": "^8.57.0",
        "helmet": "^7.1.0",
        "morgan": "^1.10.0",
        "nodemon": "^3.1.4",
        "ts-node": "^10.9.2",
        "tsc-alias": "^1.8.10",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.5.2",
        "winston": "^3.13.0"
    },
    "dependencies": {
        "@prisma/client": "^5.16.0",
        "bcrypt": "^5.1.1",
        "body-parser": "^1.20.3",
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "joi": "^17.13.3",
        "jsonwebtoken": "^9.0.2",
        "nodemailer": "^6.9.14",
        "prisma": "^5.16.0",
        "prismix": "^1.0.22"
    }
}
